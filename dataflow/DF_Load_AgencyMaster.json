{
	"name": "DF_Load_AgencyMaster",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_AgencyMaster_CSV",
						"type": "DatasetReference"
					},
					"name": "AgencyMasterSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_AgencyMaster_SQL",
						"type": "DatasetReference"
					},
					"name": "AgencyMasterSink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> AgencyMasterSource",
				"AgencyMasterSource sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> AgencyMasterSink"
			]
		}
	}
}